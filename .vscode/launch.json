  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Test",
      "skipFiles": ["<node_internals>/**"],
      "cwd": "${workspaceFolder}/tests/typedoc-plugins-example",
      "runtimeExecutable": "npx",
      "windows": {
        "runtimeExecutable": "npx.cmd"
      },
      "runtimeArgs": ["typedoc"],
      "preLaunchTask": "tsc: build - tsconfig.debug.json",
      "outFiles": [
        "${workspaceFolder}/tests/typedoc-plugins-example/node_modules/@droppedcode/typedoc-plugin-copy-assets/dist/**/*.js"
      ]
    }
  ]
}
